{
  "name": "pyicub-frontend",
  "version": "8.5.0",
  "scripts": {
    "ng": "ng",
    "config-environment": "node scripts/set-env.js",
    "create-plugin": "node scripts/create-plugin.js",
    "build-plugins": "node scripts/config-plugins.js",
    "start": "npm run build-plugins && ng serve",
    "production": "npm run config-environment && npm run build-plugins && ng serve --configuration=production --host=0.0.0.0",
