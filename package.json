    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "angular-gridster2": "^17.0.0",
    "colors": "^1.4.0",
    "d3-shape": "^3.2.0",
    "dagre": "^0.8.5",
    "dotenv": "^16.3.1",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
