{
  "name": "pyicub-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "server": "json-server -p 3000 --watch db.json",
    "config": "ts-node set-env.ts",
    "build-plugins": "ts-node config-plugins.ts",
    "start": "npm run build-plugins && (npm run server & ng serve)",
    "production": "npm run config && npm run build-plugins && ng serve --configuration=production --host=0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@swimlane/ngx-graph": "^8.2.4",
    "angular-gridster2": "^17.0.0",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "json-server": "^0.17.4",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  },
  "overrides": {
    "d3-selection": "3.0.0",
    "@swimlane/ngx-graph": {
      "@angular/animations": "~17.0.0",
      "@angular/cdk": "~17.0.0",
      "@angular/common": "~17.0.0",
      "@angular/core": "~17.0.0"
    }
  }
}
